---
layout: layouts/default.njk
pagination:
  data: collections.notesByYear
  size: 1
  alias: year
permalink: /notes/{{ year }}/
override:tags: []
eleventyComputed:
  title: "Заметки за {{ year }} год"
---

<ul class="menu menu_theme_years">
  {% for year in collections.notesByYear | objectKeys %}
    {% set url = '/notes/' + year + '/' %}
    <li>
      {% if page.url === url %}
        <span class="menu__item menu__item_selected">{{ year }}</span>
      {% else %}
        <a href="{{ url }}">{{ year }}</a>
      {% endif %}
    </li>
  {% endfor %}
</ul>

{% import "_posts.njk" as posts %}
{{ posts.list(collections.notesByYear[year]) }}
